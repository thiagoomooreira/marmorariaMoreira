<div id="main" class="main-full">
    <div class="row">
        <div class="col s12">
            <div class="container">
                <div class="row">
                    <div class="col s12">
                        <div id="icon-prefixes" class="card card-tabs">
                            <div class="card-content">
                                <div class="card-title">
                                    <div class="row">
                                        <div class="col s12 m6 l10">
                                            <h4 class="card-title">Adicionar uma Despesa</h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    @using (Html.BeginForm("Create", "Saidas", FormMethod.Post))
                                    {
                                        <!--Entrada-->
                                        <div class="row">
                                            <div class="input-field col s12 m4">
                                                <textarea id="descricao" class="materialize-textarea" name="Saida.Descricao"></textarea>
                                                <label for="descricao">Descrição</label>
                                            </div>
                                            <div class="input-field col s12 m4">
                                                <input id="valor" type="text" class="validate" name="Saida.Valor">
                                                <label for="valor">Valor</label>
                                            </div>
                                            <div class="input-field col s12 m4">
                                                <input id="data" name="Saida.Data" type="text" class="datepicker" placeholder="Data">
                                            </div>
                                        </div>
                                        <div class="divider mt-2 mb-2"></div>
                                        <div class="row">
                                            <div class="col right">
                                                <button type="submit" class="waves-effect waves-light btn">Cadastrar</button>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- END: Footer-->
